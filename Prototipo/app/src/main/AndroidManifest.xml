<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.miguel.prototipo">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.Activities.EspecificMatchDog"
            android:parentActivityName=".Activities.Activities.SearchPartnerActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activities.Activities.SearchPartnerActivity" />
        </activity>
        <activity
            android:name=".Activities.Activities.SearchPartnerActivity"
            android:parentActivityName=".Activities.Activities.MatchMainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activities.Activities.MatchMainActivity" />
        </activity>
        <activity
            android:name=".Activities.Activities.MatchMainActivity"
            android:parentActivityName=".Activities.Activities.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activities.Activities.MainActivity" />
        </activity>
        <activity
            android:name=".Activities.Activities.EditMyDog"
            android:parentActivityName=".Activities.Activities.MyDogs">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activities.Activities.MyDogs" />
        </activity>
        <activity
            android:name=".Activities.Activities.ReportDog"
            android:parentActivityName=".Activities.Activities.MyDogs">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activities.Activities.MyDogs" />
        </activity>
        <activity
            android:name=".Activities.Activities.About"
            android:parentActivityName=".Activities.Activities.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activities.Activities.MainActivity" />
        </activity>
        <activity
            android:name=".Activities.Activities.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.Activities.LogActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activities.Activities.EspecificMissingDog"
            android:label="@string/title_activity_especific_missing_dog"
            android:parentActivityName=".Activities.Activities.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activities.Activities.MainActivity" />
        </activity>
        <activity
            android:name=".Activities.Activities.MyDogs"
            android:parentActivityName=".Activities.Activities.MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activities.Activities.MainActivity" />
        </activity>
        <activity
            android:name=".Activities.Activities.AddMyDog"
            android:parentActivityName=".Activities.Activities.MyDogs">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Activities.Activities.MyDogs" />
        </activity>
    </application>

</manifest>